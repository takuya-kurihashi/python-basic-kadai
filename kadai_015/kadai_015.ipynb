      "source": [
        "# 課題 クラスを使いこなそう\n",
        "\n",
        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"名前：{self.name}\")\n",
        "    print(f\"年齢：{self.age}\")\n",
        "\n",
        "\n",
        "# 結果\n",
        "human = Human(\"課題 太郎\", 28)\n",
        "human.printinfo()"
      ]
    }
  ]
}
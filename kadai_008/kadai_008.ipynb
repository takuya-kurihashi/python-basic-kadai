      "source": [
        "# 課題 条件分岐のif文で処理を切り替えよう\n",
        "\n",
        "import random\n",
        "\n",
        "# 課題内で指定がなかったため0～100までの値とした\n",
        "var = random.randint(0, 100)\n",
        "\n",
        "if var % (3*5) == 0:\n",
        "  print(\"FizzBuzz\")\n",
        "elif var % 3 == 0:\n",
        "  print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "  print(\"Buzz\")\n",
        "else:\n",
        "  print(var)\n",
        "\n",
        "# 実際の値\n",
        "print(\"実際の値：\" + str(var))"
      ]
    }
  ]

      "source": [
        "# 課題 変数を使って四則演算しよう\n",
        "\n",
        "upper  = 10  #上辺\n",
        "lower  = 20  #下辺\n",
        "higher = 5   #高さ\n",
        "\n",
        "# 公式：台形の面積 = (上辺 + 下辺) * 高さ / 2\n",
        "result = (upper + lower) * higher / 2\n",
        "\n",
        "# 結果表示\n",
        "print(f\"{result}cm²\")"
      ]
    }
  ]
